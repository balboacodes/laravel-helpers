{
    "name": "@balboacodes/laravel-helpers",
    "version": "0.2.1",
    "description": "A TypeScript port of Laravel's helpers",
    "keywords": [
        "laravel",
        "helpers",
        "strings",
        "stringable",
        "arrays",
        "objects",
        "collections",
        "numbers"
    ],
    "homepage": "https://github.com/balboacodes/laravel-helpers#readme",
    "bugs": {
        "url": "https://github.com/balboacodes/laravel-helpers/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/balboacodes/laravel-helpers.git"
    },
    "license": "MIT",
    "author": "Balboa Codes",
    "type": "module",
    "main": "dist/index.js",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "build": "tsc && vite build",
        "format": "prettier --write .",
        "test": "vitest"
    },
    "devDependencies": {
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "vite": "^7.1.12",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@balboacodes/php-utils": "^0.8.0"
    }
}
